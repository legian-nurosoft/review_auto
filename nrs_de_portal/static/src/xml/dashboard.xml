    <t t-name="nrs_de_portal.de_dashboard_message_detail">
        <div class="de-portal-content-box">
            <span class="mb-3 de-back-to-notif-list" style="cursor: pointer;"><i class="fas fa-chevron-left"/> Back to List</span>
            <br/>
            <div class="de-ticket-log-body mt-3">
                <t t-foreach="data['message']" t-as="log">
                    <div class="de-ticket-log-item">
                        <h2 class="de-ticket-log-author">
                            <t t-esc="log['author']" />
                        </h2>
                        <h2 class="de-ticket-log-date">
                            <t t-esc="log['date']" />
                        </h2>
                        <div class="de-ticket-log-message">
                            <t t-raw="log['body']" />
                        </div>
                        <div class="de-ticket-log-attachment-box">
                            <t t-foreach="log['attachment_ids']" t-as="attachment">
                                <a class="de-ticket-log-attachment" t-att-href="attachment" target="_blank">
                                    <span class="de-ic-download" />
                                </a>
                            </t>
                        </div>
                    </div>
                </t>
                <div class="de-ticket-log-chat-container">
                    <textarea class="de-ticket-log-chat-input" id="deticketlogchatinput" rows="4"></textarea>
                    <div class="de-ticket-log-file-container"></div>
                    <button class="de-ticket-log-file-emoji">
                        <span class="fa fa-smile-o" />
                    </button>
                    <button class="de-ticket-log-file-button">
                        <span class="fa fa-paperclip" />
                        <input type="file" class="de-ticket-log-file" multiple="multiple" />
                    </button>
                    <div class="de-emoji-container de-hide"></div>
                    <button class="de-ticket-log-chat-reply" t-att-data-ref-id="data['ref_id']" t-att-data-ref-model="data['ref_model']">Send</button>
                    <button class="de-ticket-log-chat-reply-sending" t-att-data-ref-id="data['ref_id']" t-att-data-ref-model="data['ref_model']" style="display: none" disabled="disabled"><i class="fas fa-spinner de-ticket-log-chat-reply-sending-icon"></i> Sending...</button>
                </div>
            </div>
        </div>
    </t>    
    <t t-name="nrs_de_portal.de_dashboard_notification_list">
    	<div class="de-portal-subtitle-box d-flex justify-content-between align-items-center">
            <div class="d-flex text-center">
                <span class="de-portal-subicon de-ic-ticketing" />
                <span class="de-portal-subtitle">Notification List</span>
            </div>              
            <t t-call="nrs_de_portal.de_search_box_tags" />      
        </div>
        <div class="de-portal-content-box">
            <!-- <t t-call="nrs_de_portal.de_pagination" /> -->
            <div class="de-table-box">
                <table class="table table-hover">
                    <thead>
                        <tr>
                            <th>
                                Subject
                                <span class="de-portal-order fa fa-chevron-down" data-order="create_date" />
                            </th>
                            <th>
                                Notification Date Start
                                <span class="de-portal-order fa fa-chevron-down" data-order="create_uid" />
                            </th>
                            <th>
                                Notification Date End
                                <span class="de-portal-order fa fa-chevron-down" data-order="name,id" />
                            </th>
                        </tr>
                    </thead>
                    <t t-call="nrs_de_portal.de_notification_list_table_body" />
                </table>
            </div>
            <div class="clearfix"></div>
        </div>
    </t> 
    <t t-name="nrs_de_portal.de_notification_list_table_body">
        <tbody class="de-portal-notification-list-body">
            <t t-foreach="data" t-as="item">
                <tr class="de-notification-list-table-tr" t-att-data-notif-id="item['id']" style="cursor: pointer;">
                    <td>
                        <span t-esc="item['subject']" />
                    </td>
                    <td>
                        <span t-esc="item['notification_start_date']" />
                    </td>
                    <td>
                        <span t-esc="item['notification_end_date']" />
                    </td>
                </tr>
            </t>
        </tbody>
    </t>
    <t t-name="nrs_de_portal.de_dashboard_notification_detail">
        <div class="de-portal-content-box">
            <span class="mb-3 de-back-to-notif-list" style="cursor: pointer;"><i class="fas fa-chevron-left"/> Back to List</span>
            <br/>
            <h3 class="mt-3">Subject : <span t-esc="data['subject']" /></h3>
            <h3>Notifiaction Date : <i><span t-esc="data['notification_start_date']" /></i> to <i><span t-esc="data['notification_end_date']" /></i></h3>
            <div>
                <t t-raw="data['body']" />
            </div>
        </div>
    </t> 
</templates>