<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="res_company_form_view1">
        <field name="name">res_company_form_view1</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form" />
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[@name='general_info']" position="after">
                <page string="MISC" name="misc_info">
                    <group>
                        <group>
                            <field name="ns_expense_required_tax"/>
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>